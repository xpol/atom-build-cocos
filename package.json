{
  "name": "build-cocos",
  "version": "0.1.0",
  "description": "Runs cocos comand to build cocos2d-x C++/Lua/Web projects",
  "repository": "https://github.com/xpol/build-cocos2d-x",
  "license": "MIT",
  "main": "lib/cocos.js",
  "engines": {
    "atom": "^1.0.0"
  },
  "providedServices": {
    "builder": {
      "versions": {
        "2.0.0": "provideBuilder"
      }
    }
  },
  "devDependencies": {
    "babel-eslint": "^4.1.5",
    "eslint": "^1.10.1",
    "eslint-config-atom-build": "^1.0.0"
  },
  "keywords": [
    "build",
    "compile",
    "cocos2d-x",
    "c++",
    "lua",
    "js",
    "productivity"
  ],
  "eslintConfig": {
    "env": {
      "node": true
    },
    "parser": "babel-eslint",
    "extends": "atom-build",
    "rules": {
      "no-unused-vars": [
        2,
        {
          "vars": "all",
          "args": "all"
        }
      ],
      "quotes": [
        2,
        "double"
      ],
      "space-after-keywords": 2,
      "space-before-blocks": 2,
      "space-before-function-paren": [
        2,
        "never"
      ],
      "space-in-parens": [
        2,
        "never"
      ],
      "strict": [
        2,
        "global"
      ]
    }
  }
}
